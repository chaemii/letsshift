    private func calculateTodayShiftFromPattern() -> ShiftType {
        let userDefaults = UserDefaults(suiteName: "group.com.chaeeun.ShiftCalendarApp")!
        let settingsKey = "shiftSettings"
        
        print("âš™ï¸ Widget Debug - Starting pattern calculation...")
        
        if let data = userDefaults.data(forKey: settingsKey) {
            print("âš™ï¸ Widget Debug - Found settings data: \(data.count) bytes")
            if let settings = try? JSONDecoder().decode(ShiftSettingsData.self, from: data) {
                print("âš™ï¸ Widget Debug - Pattern: \(settings.shiftPatternType.rawValue), Team: \(settings.team)")
                print("âš™ï¸ Widget Debug - Custom pattern exists: \(settings.customPattern != nil)")
                if let customPattern = settings.customPattern {
                    print("âš™ï¸ Widget Debug - Custom pattern: \(customPattern.dayShifts.map { $0.rawValue })")
                    print("âš™ï¸ Widget Debug - Custom pattern start date: \(customPattern.startDate)")
                }
                
                // í˜„ì¬ ì„¤ì •ëœ íŒ¨í„´ì— ë”°ë¼ ì˜¤ëŠ˜ ê·¼ë¬´ ê³„ì‚°
                let today = Date()
                let calendar = Calendar.current
                
                print("âš™ï¸ Widget Debug - Today's date: \(today)")
                
                // íŒ¨í„´ íƒ€ì…ì— ë”°ë¥¸ ê·¼ë¬´ ê³„ì‚°
                let patternType = settings.shiftPatternType.rawValue
                let team = settings.team
                let calculatedShift = calculateShiftFromPattern(patternType: patternType, date: today, team: team)
                print("âš™ï¸ Widget Debug - Calculated shift: \(calculatedShift.rawValue)")
                return calculatedShift
            } else {
                print("âŒ Widget Debug - Failed to decode settings")
            }
        } else {
            print("âš™ï¸ Widget Debug - No settings data found")
        }
        
        print("âŒ Widget Debug - Returning default: íœ´ë¬´")
        return .íœ´ë¬´
    }
    
    private func calculateShiftFromPattern(patternType: String, date: Date, team: String) -> ShiftType {
        let calendar = Calendar.current
        
        // íŒ€ ë²ˆí˜¸ ì¶”ì¶œ (ì˜ˆ: "1ì¡°" -> 1)
        let teamNumber = Int(team.replacingOccurrences(of: "ì¡°", with: "")) ?? 1
        
        // shiftOffset ê°€ì ¸ì˜¤ê¸°
        let userDefaults = UserDefaults(suiteName: "group.com.chaeeun.ShiftCalendarApp")!
        let shiftOffset = userDefaults.integer(forKey: "shiftOffset")
        
        print("ğŸ”§ Widget Debug - Pattern calculation: patternType=\(patternType), team=\(team), teamNumber=\(teamNumber), shiftOffset=\(shiftOffset)")
        
        // ë©”ì¸ ì•±ê³¼ ë™ì¼í•œ ë¡œì§ìœ¼ë¡œ ê³„ì‚°
        switch patternType {
        case "twoShift":
            // 2êµëŒ€: ì£¼ê°„-ì•¼ê°„
            let pattern = [ShiftType.ì£¼ê°„, ShiftType.ì•¼ê°„]
            let dayOfYear = calendar.ordinality(of: .day, in: .year, for: date) ?? 1
            let adjustedDayOfYear = dayOfYear + shiftOffset
            let teamOffset = (teamNumber - 1)
            let adjustedDay = (adjustedDayOfYear + teamOffset) % pattern.count
            let positiveIndex = adjustedDay >= 0 ? adjustedDay : pattern.count + adjustedDay
            let result = pattern[positiveIndex % pattern.count]
            print("ğŸ”§ Widget Debug - TwoShift: dayOfYear=\(dayOfYear), adjustedDayOfYear=\(adjustedDayOfYear), teamOffset=\(teamOffset), adjustedDay=\(adjustedDay), positiveIndex=\(positiveIndex), result=\(result.rawValue)")
            return result
            
        case "threeShift":
            // 3êµëŒ€: ì£¼ê°„-ì•¼ê°„-ë¹„ë²ˆ
            let pattern = [ShiftType.ì£¼ê°„, ShiftType.ì•¼ê°„, ShiftType.ë¹„ë²ˆ]
            let dayOfYear = calendar.ordinality(of: .day, in: .year, for: date) ?? 1
            let adjustedDayOfYear = dayOfYear + shiftOffset
            let teamOffset = (teamNumber - 1)
            let adjustedDay = (adjustedDayOfYear + teamOffset) % pattern.count
            let positiveIndex = adjustedDay >= 0 ? adjustedDay : pattern.count + adjustedDay
            let result = pattern[positiveIndex % pattern.count]
            print("ğŸ”§ Widget Debug - ThreeShift: dayOfYear=\(dayOfYear), adjustedDayOfYear=\(adjustedDayOfYear), teamOffset=\(teamOffset), adjustedDay=\(adjustedDay), positiveIndex=\(positiveIndex), result=\(result.rawValue)")
            return result
            
        case "threeTeamTwoShift":
            // 3ì¡° 2êµëŒ€: ì£¼ê°„-ì•¼ê°„-íœ´ë¬´
            let pattern = [ShiftType.ì£¼ê°„, ShiftType.ì•¼ê°„, ShiftType.íœ´ë¬´]
            let dayOfYear = calendar.ordinality(of: .day, in: .year, for: date) ?? 1
            let adjustedDayOfYear = dayOfYear + shiftOffset
            let teamOffset = (teamNumber - 1)
            let adjustedDay = (adjustedDayOfYear + teamOffset) % pattern.count
            let positiveIndex = adjustedDay >= 0 ? adjustedDay : pattern.count + adjustedDay
            let result = pattern[positiveIndex % pattern.count]
            print("ğŸ”§ Widget Debug - ThreeTeamTwoShift: dayOfYear=\(dayOfYear), adjustedDayOfYear=\(adjustedDayOfYear), teamOffset=\(teamOffset), adjustedDay=\(adjustedDay), positiveIndex=\(positiveIndex), result=\(result.rawValue)")
            return result
            
        case "fourTeamTwoShift":
            // 4ì¡° 2êµëŒ€: ì£¼ê°„-ì•¼ê°„-ë¹„ë²ˆ-íœ´ë¬´
            let pattern = [ShiftType.ì£¼ê°„, ShiftType.ì•¼ê°„, ShiftType.ë¹„ë²ˆ, ShiftType.íœ´ë¬´]
            let dayOfYear = calendar.ordinality(of: .day, in: .year, for: date) ?? 1
            let adjustedDayOfYear = dayOfYear + shiftOffset
            let teamOffset = (teamNumber - 1)
            let adjustedDay = (adjustedDayOfYear + teamOffset) % pattern.count
            let positiveIndex = adjustedDay >= 0 ? adjustedDay : pattern.count + adjustedDay
            let result = pattern[positiveIndex % pattern.count]
            print("ğŸ”§ Widget Debug - FourTeamTwoShift: dayOfYear=\(dayOfYear), adjustedDayOfYear=\(adjustedDayOfYear), teamOffset=\(teamOffset), adjustedDay=\(adjustedDay), positiveIndex=\(positiveIndex), result=\(result.rawValue)")
            return result
            
        case "fourTeamThreeShift":
            // 4ì¡° 3êµëŒ€: ì£¼ê°„-ì˜¤í›„-ì•¼ê°„-íœ´ë¬´
            let pattern = [ShiftType.ì£¼ê°„, ShiftType.ì˜¤í›„, ShiftType.ì•¼ê°„, ShiftType.íœ´ë¬´]
            let dayOfYear = calendar.ordinality(of: .day, in: .year, for: date) ?? 1
            let adjustedDayOfYear = dayOfYear + shiftOffset
            let teamOffset = (teamNumber - 1)
            let adjustedDay = (adjustedDayOfYear + teamOffset) % pattern.count
            let positiveIndex = adjustedDay >= 0 ? adjustedDay : pattern.count + adjustedDay
            let result = pattern[positiveIndex % pattern.count]
            print("ğŸ”§ Widget Debug - FourTeamThreeShift: dayOfYear=\(dayOfYear), adjustedDayOfYear=\(adjustedDayOfYear), teamOffset=\(teamOffset), adjustedDay=\(adjustedDay), positiveIndex=\(positiveIndex), result=\(result.rawValue)")
            return result
            
        case "fiveTeamThreeShift":
            // 5ì¡° 3êµëŒ€: ì£¼ê°„-ì•¼ê°„-ì‹¬ì•¼-ë¹„ë²ˆ-íœ´ë¬´
            let pattern = [ShiftType.ì£¼ê°„, ShiftType.ì•¼ê°„, ShiftType.ì‹¬ì•¼, ShiftType.ë¹„ë²ˆ, ShiftType.íœ´ë¬´]
            let dayOfYear = calendar.ordinality(of: .day, in: .year, for: date) ?? 1
            let adjustedDayOfYear = dayOfYear + shiftOffset
            let teamOffset = (teamNumber - 1)
            let adjustedDay = (adjustedDayOfYear + teamOffset) % pattern.count
            let positiveIndex = adjustedDay >= 0 ? adjustedDay : pattern.count + adjustedDay
            let result = pattern[positiveIndex % pattern.count]
            print("ğŸ”§ Widget Debug - FiveTeamThreeShift: dayOfYear=\(dayOfYear), adjustedDayOfYear=\(adjustedDayOfYear), teamOffset=\(teamOffset), adjustedDay=\(adjustedDay), positiveIndex=\(positiveIndex), result=\(result.rawValue)")
            return result
            
        case "custom":
            // ì»¤ìŠ¤í…€ íŒ¨í„´ ì²˜ë¦¬
            return calculateCustomPatternShift(date: date, team: team)
            
        default:
            return .íœ´ë¬´
        }
    }
    
    private func calculateCustomPatternShift(date: Date, team: String) -> ShiftType {
        let userDefaults = UserDefaults(suiteName: "group.com.chaeeun.ShiftCalendarApp")!
        let settingsKey = "shiftSettings"
        
        guard let data = userDefaults.data(forKey: settingsKey),
              let settings = try? JSONDecoder().decode(ShiftSettingsData.self, from: data),
              let customPattern = settings.customPattern else {
            print("âŒ Widget Debug - Custom pattern not found")
            return .íœ´ë¬´
        }
        
        print("ğŸ”§ Widget Debug - Custom pattern found: \(customPattern.dayShifts)")
        
        let calendar = Calendar.current
        let startOfDay = calendar.startOfDay(for: customPattern.startDate)
        let targetStartOfDay = calendar.startOfDay(for: date)
        let daysFromStart = calendar.dateComponents([.day], from: startOfDay, to: targetStartOfDay).day ?? 0
        
        // ì‹œì‘ì¼ ì´ì „ì—ëŠ” íœ´ë¬´ ë°˜í™˜
        if daysFromStart < 0 {
            print("ğŸ”§ Widget Debug - Before start date, returning íœ´ë¬´")
            return .íœ´ë¬´
        }
        
        let shiftPattern = customPattern.dayShifts
        let teamNumber = Int(team.replacingOccurrences(of: "ì¡°", with: "")) ?? 1
        let shiftOffset = userDefaults.integer(forKey: "shiftOffset")
        
        // íŒ€ë³„ë¡œ ê·¼ë¬´ê°€ í•˜ë‚˜ì”© ë°€ë ¤ì„œ ì—‡ê°ˆë¦¬ê²Œ êµ¬ì„±
        let teamOffset = (teamNumber - 1)
        let adjustedDay = daysFromStart + teamOffset + shiftOffset
        let patternIndex = adjustedDay % shiftPattern.count
        let positiveIndex = patternIndex >= 0 ? patternIndex : shiftPattern.count + patternIndex
        
        print("ğŸ”§ Widget Debug - Custom pattern calculation: daysFromStart=\(daysFromStart), teamOffset=\(teamOffset), shiftOffset=\(shiftOffset), adjustedDay=\(adjustedDay), patternIndex=\(patternIndex), positiveIndex=\(positiveIndex)")
        
        return shiftPattern[positiveIndex % shiftPattern.count]
    }
}

